    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "prettier": "^1.18.2",
    "tap": "^12.7.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/hosted-git-info": "^2.7.0",
    "@types/node": "^6.14.7",
    "debug": "^3.1.0",
